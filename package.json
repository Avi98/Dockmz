{
  "name": "mono-review",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/node": "^18.14.5",
    "@types/tar": "^6.1.4",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "tar": "^6.1.13",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.21.0",
  "dependencies": {
    "dotenv": "^16.0.3"
  }
}
